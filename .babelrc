{
	"presets": [
		[
			"env",
			{
				"modules": false
			}
		],
		"stage-2"
	],
	"plugins": [
		[
			"transform-runtime",
			{
				"helpers": false,
				"polyfill": false,
				"regenerator": true,
				"moduleName": "babel-runtime"
			}
		],
		[
			"component",
			[
				{
					"libraryName": "element-ui",
					"styleLibraryName": "theme-chalk"
				}
			]
		],
		"syntax-dynamic-import"
	]
}